          <TableColumn fx:id="proQty5Col" prefWidth="90.0" text="工序5实作数量" />
          <TableColumn fx:id="pro6Col" prefWidth="90.0" text="工序6" />
          <TableColumn fx:id="proQty6Col" prefWidth="90.0" text="工序6实作数量" />
          <TableColumn fx:id="createUserCol" prefWidth="90.0" text="创建人员" />
          <TableColumn fx:id="createTimeCol" prefWidth="140.0" text="创建时间" />
          <TableColumn fx:id="modifyUserCol" prefWidth="90.0" text="修改人员" />
          <TableColumn fx:id="modifyTimeCol" prefWidth="140.0" text="修改时间" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="UNCONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <GridPane prefHeight="17.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="797.0" minWidth="10.0" prefWidth="740.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="65.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="6.0" prefWidth="66.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="193.0" minWidth="10.0" prefWidth="32.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="当前用户：" GridPane.columnIndex="2" />
            <Label fx:id="currentUser" text=" " GridPane.columnIndex="3" />
         </children>
      </GridPane>
